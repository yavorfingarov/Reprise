    </None>
    <None Update="EndpointRouteBuilderExtensions.MapEndpointsAssembly_AppNull.received.txt">
      <DependentUpon>EndpointRouteBuilderExtensions.cs</DependentUpon>
    </None>
    <None Update="EndpointRouteBuilderExtensions.MapEndpointsAssembly_AssemblyNull.received.txt">
      <DependentUpon>EndpointRouteBuilderExtensions.cs</DependentUpon>
    </None>
    <None Update="Reflection.PublicTypes.verified.txt">
